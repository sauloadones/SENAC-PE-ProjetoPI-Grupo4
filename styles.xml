<resources>

    <style name="AppTheme" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="colorPrimary">#164773</item>
        <item name="colorPrimaryVariant">#0F355A</item>
        <item name="colorOnPrimary">#FFFFFF</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>

    <style name="LoginDialogTheme" parent="Theme.MaterialComponents.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="BottomNavActiveIndicator">
        <item name="android:color">#3CA6A6</item>
        <item name="android:shape">rectangle</item>
        <item name="android:height">4dp</item>
    </style>

    <style name="button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">40dp</item>
        <item name="android:layout_marginRight">4dp</item>
        <item name="android:layout_marginTop">15dp</item>
        <item name="android:background">@drawable/button</item>


    </style>


</resources>




